{
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeInternal": true,
    "disableSources": true,
    "sort": ["source-order", "instance-first"],
    "readme": "none",
    "theme": "markdown",
    "plugin": ["typedoc-plugin-merge-modules", "typedoc-plugin-markdown"],
    "entryPointStrategy": "expand",
    "entryPoints": [ "./src"],
    "out": "./docs"
}
